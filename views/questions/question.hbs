<div class="container">
  <div class="Question">
    <h2> Question </h2><h3>{{q}}</h3> <br>

    <form class="form" action="" method="post">
      <ul>
        {{#each a}}
        <li><input name="answer" type="radio" value="{{this}}">{{this}}</li>
        {{/each}}
      </ul>
      <input type="submit"/>
    </form>
       <img src="{{img}}" style = "width: 500px">
  </div>
</div>
